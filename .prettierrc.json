{
  "$schema": "https://json.schemastore.org/prettierrc",
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 100,
  "trailingComma": "es5",
  "plugins": ["prettier-plugin-css-order", "@trivago/prettier-plugin-sort-imports"],
  "overrides": [
    {
      "files": "*.cjs",
      "options": { "parser": "babel" }
    },
    {
      "files": "*.svg",
      "options": {
        "parser": "html"
      }
    }
  ]
}
